_batchManager() {

    local cur opts

    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"

    if [[ ${cur} == * ]] ; then
        opts="help submit stop restart status getJobLog listJobs purge"
        COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
        return 0
    fi

    return 0
}

complete -F _batchManager batchManager
